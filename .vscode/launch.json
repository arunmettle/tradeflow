{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Next.js: dev",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect", "node_modules/.bin/next", "dev"],
      "cwd": "${workspaceFolder}/tradeflow",
      "console": "integratedTerminal",
      "env": {
        "NODE_OPTIONS": "--enable-source-maps"
      },
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
